      "cell_type": "code",
      "source": [
        "from transformers import BartTokenizer, BartForConditionalGeneration, Trainer, TrainingArguments\n",
        "import torch\n",
        "\n",
        "class DenoisingDataset(torch.utils.data.Dataset):\n",
        "    def __init__(self, encodings):\n",
        "        self.encodings = encodings\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        return {key: torch.tensor(val[idx]) for key, val in self.encodings.items()}\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.encodings.input_ids)\n",
        "\n",
        "tokenizer = BartTokenizer.from_pretrained('facebook/bart-large')\n",
        "\n",
        "def read_dataset(file_path):\n",
        "    with open(file_path, 'r', encoding='utf-8') as file:\n",
        "        lines = file.readlines()\n",
        "    original_texts, denoised_texts = zip(*(line.strip().split('\\t') for line in lines))\n",
        "    return original_texts, denoised_texts\n",
        "\n",
        "original_texts, denoised_texts = read_dataset('/content/drive/MyDrive/OCR/denoised_dataset.txt')\n",
        "encodings = tokenizer(denoised_texts, truncation=True, padding='max_length', max_length=512)\n",
        "decodings = tokenizer(original_texts, truncation=True, padding='max_length', max_length=512)\n",
        "dataset = DenoisingDataset(encodings)\n"
      ],
      "metadata": {
        "id": "MjeghM5xOW9S"
      },
      "execution_count": 2,
      "outputs": []
    },
